pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Bye') {
            steps {
                echo 'Bye World'
            }
        }
        
        stage('Tata') {
            steps {
                echo 'Tata World'
            }
        }
    }
}
